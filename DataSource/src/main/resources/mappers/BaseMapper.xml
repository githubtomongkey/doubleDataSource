<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leyou.item.mapper.BaseMapper">

    <select id="dual" resultType="int">
        select count(*) from dept
    </select>

    <update id="update">
        update dept set dname='开发' where deptno='1'
    </update>
</mapper>
